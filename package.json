{
  "name": "zync",
  "private": true,
  "version": "1.0.0",
  "description": "Secure clipboard syncing between your devices.",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome && npm run build:firefox && npm run build:opera",
    "build:chrome": "gulp --production --vendor=chrome && gulp pack --vendor=chrome",
    "build:firefox": "gulp --production --vendor=firefox && gulp pack --vendor=firefox",
    "build:opera": "gulp --production --vendor=opera && gulp pack --vendor=opera",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera"
  },
  "dependencies": {
    "firebase": "^4.1.3",
    "webextension-polyfill": "^0.1.1",
    "base64-arraybuffer": "0.1.5"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-eslint": "7.x.x",
    "babel-loader": "7.x.x",
    "babel-preset-env": "1.x.x",
    "babel-preset-es2015": "^6.24.1",
    "chai": "4.x.x",
    "chromereload": "0.x.x",
    "debounce": "1.x.x",
    "del": "3.x.x",
    "eslint": "^4.x.x",
    "eslint-loader": "1.x.x",
    "gulp": "3.x.x",
    "gulp-bump": "2.x.x",
    "gulp-cache": "0.x.x",
    "gulp-clean-css": "^3.x.x",
    "gulp-filter": "^5.x.x",
    "gulp-git": "^2.x.x",
    "gulp-if": "2.x.x",
    "gulp-imagemin": "3.x.x",
    "gulp-json-transform": "0.x.x",
    "gulp-livereload": "3.x.x",
    "gulp-plumber": "1.x.x",
    "gulp-sass": "^3.x.x",
    "gulp-sequence": "0.x.x",
    "gulp-sourcemaps": "^2.x.x",
    "gulp-tag-version": "1.x.x",
    "gulp-util": "3.x.x",
    "gulp-zip": "^4.x.x",
    "require-dir": "0.x.x",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "vinyl-named": "1.x.x",
    "webpack": "3.x.x",
    "webpack-stream": "3.x.x",
    "yargs": "^8.x.x"
  }
}
